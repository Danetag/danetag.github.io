{
	"name": "mood-mixer-auth",
	"description": "Mood Mixer - Authentification",
	"version": "0.0.1",
	"private": true,
	"author": "Arnaud Tanielian",
	"repository": {
		"type": "git",
		"url": "https://github.com/danetag/danetag.github.io"
	},
	"bugs": {
		"url": "https://github.com/danetag/danetag.github.io"
	},
	"app": {},
	"engines": {
    "node": ">=7.0.0",
    "npm": ">=4.1.2"
  },
	"dependencies": {
		"detectizr": "git://github.com/barisaydinoglu/Detectizr.git#v2.2.0",
		"preload-js": "^0.6.3"
	},
	"devDependencies": {
		"ampersand-router": "4.0.0",
		"autoprefixer": "6.7.2",
		"babel-core": "6.18.2",
    "babel-eslint": "7.1.0",
    "babel-loader": "6.2.7",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "6.18.0",
		"concurrently": "^2.2.0",
		"customizr": "1.0.0-alpha",
		"del-cli": "^0.2.0",
		"es6-promise": "^4.0.5",
		"eslint": "^3.9.1",
		"eslint-config-google": "^0.6.0",
		"eslint-loader": "^1.5.0",
		"exports-loader": "^0.6.3",
		"gsap": "1.19.0",
		"handlebars": "^4.0.6",
		"handlebars-loader": "^1.4.0",
		"imports-loader": "^0.6.5",
		"json-loader": "^0.5.4",
		"livereload": "^0.6.0",
		"merge-stream": "^1.0.0",
		"node-sass": "4.5.0",
		"node-sass-import": "1.1.1",
		"normalize.css": "^5.0.0",
		"OneDollar.js": "2.0.0",
		"p5": "0.5.7",
		"parallelshell": "latest",
		"postcss-cli": "2.6.0",
		"raw-loader": "0.5.1",
    "redux": "^3.5.2",
    "redux-logger": "^2.7.4",
    "redux-promise-middleware": "^4.2.0",
    "redux-responsive": "^4.1.1",
		"redux-watch": "^1.1.1",
		"run-sequence": "1.1.5",
		"source-map": "0.5.3",
		"underscore": "1.8.3",
		"vinyl-buffer": "1.0.0",
		"watch": "latest",
		"webpack": "1.14.0",
		"webpack-zepto": "0.0.1",
		"whatwg-fetch": "2.0.3"
	},
	"scripts": {
		"modernizr": "$(npm bin)/customizr -c ./customizr.config.json",
		"dev": "npm run modernizr && npm run dev:css && concurrently --kill-others 'livereload assets/css -d -e css' 'npm run dev:watch-css' 'npm run dev:js' ",
    "dev:css": "$(npm bin)/node-sass ./css/app.scss ./assets/css/app.css --importer node_modules/node-sass-import && npm run dev:postcss",
    "dev:postcss": "$(npm bin)/postcss --use autoprefixer --map -c ./autoprefixer-options.json -o ./assets/css/app.css ./assets/css/app.css",
		"dev:watch-css": "watch \"npm run dev:css\" css",
    "dev:js": "webpack --config ./webpack.config.js --progress --colors --watch --cache",
		"build": "npm run modernizr && npm run build:css && npm run build:js",
		"build:css": "$(npm bin)/node-sass --output-style compressed ./css/app.scss ./assets/css/app.min.css --importer node_modules/node-sass-import && npm run build:postcss",
		"build:postcss": "$(npm bin)/postcss --use autoprefixer -c ./autoprefixer-options.json -o ./assets/css/app.min.css ./assets/css/app.min.css",
		"build:js": "webpack --config ./webpack.production.config.js --progress --colors"
	}
}
